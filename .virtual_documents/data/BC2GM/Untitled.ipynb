





def get_prf(gold_label_list, pred_label_list):
    P, R, C, F = 0, 0, 0, 0

    for gold_label, pred_label in zip(gold_label_list, pred_label_list):
        gold_label = [gl.strip() for gl in gold_label]
        pred_label = [pl.strip() for pl in pred_label]
        gold_label = set(gold_label)
        pred_label = set(pred_label)
        P += len(pred_label)
        R += len(gold_label)
        C += len(pred_label & gold_label)
    P = C / P if P > 0 else 0
    R = C / R if R > 0 else 0
    F = 2 * P * R / (P + R) if P + R > 0 else 0
    return P, R, F





gold_labels = [['scr1 - 1$GENE', 'AEP$GENE'], ['Rep78$GENE', 'Rep68$GENE'], ['TAR - binding protein$GENE', 'CREB2 protein$GENE'], ['None$None']]
pred_labels = [['scr1 - 1 mutation$GENE', 'AEP$GENE'], ['Rep78$GENE', 'Rep68$GENE'], ['TAR - binding protein$GENE', 'CREB2 protein$GENE'], ['None$None']]


get_prf(gold_labels, pred_labels)


















